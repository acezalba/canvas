{% style priority: 30 %}
:root {
    --mono-font: Consolas, Menlo, Monaco, "Andale Mono", "Ubuntu Mono", monospace;
    --bg: {{ theme_config.color.custom.light.bg }};
    --accent-bg: {{ theme_config.color.custom.light.accent_bg }};
    --text: {{ theme_config.color.custom.light.text }};
    --text-light: {{ theme_config.color.custom.light.text_light }};
    --border: {{ theme_config.color.custom.light.border }};
    --accent: {{ theme_config.color.custom.light.accent }};
    --code: {{ theme_config.color.custom.light.code }};
    --preformatted: {{ theme_config.color.custom.light.preformatted }};
    --marked: {{ theme_config.color.custom.light.marked }};
    --disabled: {{ theme_config.color.custom.light.disabled }};
}
{% endstyle %}
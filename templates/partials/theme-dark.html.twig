{% style priority: 30 %}
:root {
    color-scheme: dark;
    --bg: {{ theme_config.color.custom.dark.bg }};
    --accent-bg: {{ theme_config.color.custom.dark.accent_bg }};
    --text: {{ theme_config.color.custom.dark.text }};
    --text-light: {{ theme_config.color.custom.dark.text_light }};
    --border: {{ theme_config.color.custom.dark.border }};
    --accent: {{ theme_config.color.custom.dark.accent }};
    --code: {{ theme_config.color.custom.dark.code }};
    --preformatted: {{ theme_config.color.custom.dark.preformatted }};
    --disabled: {{ theme_config.color.custom.dark.disabled }};
}
img,
video {
    opacity: 0.8;
    }
}
{% endstyle %}